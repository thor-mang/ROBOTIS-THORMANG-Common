          <axis xyz="0 1 0" />		
          <origin xyz="0.0 ${0.177-offset} 0.16" rpy="0 0 0"/>
          <parent link="${parent}"/>
          <child link="l_arm_sh_p1_link"/>
          <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
          <dynamics damping="0.1" friction="0.0" />
