<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">	

    <xacro:property name="pi" value="3.141592654" />
    <xacro:property name="offset" value="0.0" />

    <xacro:arg name="use_hands" default="false"/>

    <!-- VT hands description -->
    <xacro:if value="$(arg use_hands)">
        <xacro:include filename="$(find thor_vt_hand_description)/urdf/vt_hand_tud.urdf.xacro.xml" />

        <vt_hand_tud_macro parent="l_arm_wr_p_link" prefix="l" reflect="-1" >
            <origin xyz="0.068 0.045 0" rpy="0 -1.57 3.14159"/>
        </vt_hand_tud_macro>

        <vt_hand_tud_macro parent="r_arm_wr_p_link" prefix="r" reflect="1" >
            <origin xyz="0.068 -0.045 0" rpy="0 1.57 0"/>
        </vt_hand_tud_macro>
    </xacro:if>

